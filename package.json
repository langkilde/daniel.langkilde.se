{
  "name": "daniel.langkilde.se",
  "version": "3.0.0",
  "description": "personal landing page for daniel langkilde",
  "author": "Daniel Langkilde <daniel.langkilde@gmail.com> (https://daniel.langkilde.se/)",
  "homepage" : "https://www.daniel.langkilde.se",
  "repository": "https://github.com/langkilde/daniel.langkilde.se.git",
  "license": "MIT",
  "main": "build/app.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "yarn run clean && node ./node_modules/webpack/bin/webpack.js -p --config configs/webpack.config.js --env.build --env.sourceMap",
    "build:dash": "yarn run clean && webpack-dashboard -- webpack -p --config configs/webpack.config.js --env.build --env.sourceMap",
    "dev": "webpack-dev-server --config configs/webpack.config.js",
    "dev:open": "webpack-dev-server --config configs/webpack.config.js --open",
    "dev:dash": "webpack-dashboard -m -- webpack-dev-server --config configs/webpack.config.js --open",
    "dev2": "webpack-dashboard -m -- webpack-dev-server --config configs/webpack.config.js --open --env.awesome",
    "lint": "tslint --project tsconfig.json && echo \"running stylelint\" &&./node_modules/stylelint/bin/stylelint.js \"src/**/*.scss\"",
    "tsc": "tsc -p . --noEmit",
    "tsc:watch": "tsc -p . --noEmit -w",
    "test": "jest --config jest.json",
    "test:update": "jest --config jest.json -u",
    "reinstall": "rm -rf node_modules && yarn",
    "precommit": "yarn run lint",
    "prepush": "yarn run lint & yarn run tsc & yarn run test"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router-dom": "^4.1.2",
    "redux": "^3.6.0",
    "tslib": "^1.6.1",
    "prop-types": "^15.5.10"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/chai-as-promised": "0.0.31",
    "@types/enzyme": "^2.8.4",
    "@types/jest": "^20.0.4",
    "@types/node": "^8.0.15",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^15.0.6",
    "@types/react-dom": "^15.5.0",
    "@types/react-hot-loader": "^3.0.1",
    "@types/webpack": "^3.0.5",
    "@types/webpack-dev-server": "^2.4.0",
    "@types/webpack-env": "^1.13.0",
    "awesome-typescript-loader": "^3.1.3",
    "babel-core": "^6.2.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.14.3",
    "image-webpack-loader": "^3.3.1",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "loglevel": "^1.4.1",
    "node-sass": "^4.5.3",
    "react-create-class": "^1.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "ts-jest": "^20.0.7",
    "ts-loader": "^2.0.3",
    "tslint": "^5.1.0",
    "tslint-react": "^3.0.0",
    "typescript": "^2.3.2",
    "webpack": "^3.3.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.3.0"
  }
}

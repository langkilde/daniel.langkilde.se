{
  "name": "daniel.langkilde.se",
  "version": "3.0.0",
  "description": "personal landing page for daniel langkilde",
  "author": "Daniel Langkilde <daniel.langkilde@gmail.com> (https://daniel.langkilde.se/)",
  "homepage": "https://www.daniel.langkilde.se",
  "repository": "https://github.com/langkilde/daniel.langkilde.se.git",
  "license": "MIT",
  "main": "build/app.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "yarn run clean && node ./node_modules/webpack/bin/webpack.js -p --config configs/webpack.config.js --env.build --env.sourceMap",
    "build:dash": "yarn run clean && webpack-dashboard -- webpack -p --config configs/webpack.config.js --env.build --env.sourceMap",
    "dev": "webpack-dev-server --config configs/webpack.config.js",
    "dev:open": "webpack-dev-server --config configs/webpack.config.js --open",
    "dev:dash": "webpack-dashboard -m -- webpack-dev-server --config configs/webpack.config.js --open",
    "dev2": "webpack-dashboard -m -- webpack-dev-server --config configs/webpack.config.js --open --env.awesome",
    "lint": "tslint --project tsconfig.json && echo \"running stylelint\" &&./node_modules/stylelint/bin/stylelint.js \"src/**/*.scss\"",
    "tsc": "tsc -p .",
    "tsc:watch": "tsc -p . --noEmit -w",
    "test": "jest --config jest.json",
    "test:update": "jest --config jest.json -u",
    "reinstall": "rm -rf node_modules && yarn",
    "precommit": "yarn run lint",
    "prepush": "yarn run lint & yarn run tsc & yarn run test"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "cors": "^2.8.4",
    "history": "^4.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "ts-node": "^3.3.0",
    "tslib": "^1.7.1"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/chai-as-promised": "7.1.0",
    "@types/enzyme": "^2.8.11",
    "@types/history": "^4.6.0",
    "@types/jest": "^21.1.2",
    "@types/node": "^8.0.32",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.9",
    "@types/react-dom": "^16.0.0",
    "@types/react-hot-loader": "^3.0.4",
    "@types/react-redux": "^5.0.10",
    "@types/react-router-dom": "^4.0.8",
    "@types/webpack": "^3.0.13",
    "@types/webpack-dev-server": "^2.4.2",
    "@types/webpack-env": "^1.13.1",
    "awesome-typescript-loader": "^3.2.3",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "express": "^4.16.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "image-webpack-loader": "^3.4.2",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "loglevel": "^1.5.0",
    "morgan": "^1.9.0",
    "node-sass": "^4.5.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "ts-jest": "^21.1.0",
    "ts-loader": "^2.3.7",
    "tslint": "^5.7.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.5.3",
    "webpack": "^3.6.0",
    "webpack-dashboard": "^1.0.0-7",
    "webpack-dev-server": "^2.9.1"
  }
}
